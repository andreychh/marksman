    <center>
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="380.0" spacing="10.0">
            <children>
                <Label text="name:"/>
                <TextField fx:id="nameTextField" prefHeight="25.0" prefWidth="120.0"/>
